{
	"compilerOptions": {
		"baseUrl": "src",
		"outDir": "build/dist",
		"module": "es2015",
		"skipLibCheck": true,
		"target": "es6",
		"lib": ["dom", "es6"],
		"sourceMap": true,
		"allowJs": true,
		"jsx": "react",
		"moduleResolution": "node",
		"rootDir": "src",
		"forceConsistentCasingInFileNames": true,
		"noImplicitReturns": true,
		"noImplicitThis": true,
		"noImplicitAny": true,
		"importHelpers": true,
		"strictNullChecks": true,
		"suppressImplicitAnyIndexErrors": true,
		"noUnusedLocals": false,
		"downlevelIteration": true,
		"noUnusedParameters": false,
		"paths": {
			"Data/*": ["../../Data/*"],
			"Core/*": ["../../Core/*"]
		}
	},
	"exclude": [
		"node_modules",
		"build",
		"scripts",
		"webpack",
		"jest",
		"../../setupTests.ts"
	]
}
